# IPPOC Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://localhost/ippoc
REDIS_URL=redis://localhost:6379

# Network Configuration
IPPOC_PORT=8080
MESH_PORT=9000

# Component Toggles
ENABLE_NETWORK=true
ENABLE_MEMORY=true
ENABLE_COGNITION=true
ENABLE_ECONOMY=true

# Logging
RUST_LOG=info
LOG_LEVEL=INFO